# SAT/ACT Notes Organizer - Environment Configuration
# Copy this file to .env and fill in your values

# AI Service Configuration
AI_API_KEY=your_ai_api_key_here
AI_BASE_URL=https://api-inference.modelscope.cn/v1/chat/completions
AI_MODEL=Qwen/Qwen3-235B-A22B-Thinking-2507

# OCR Configuration (optional - will use defaults if not set)
TESSERACT_CMD=/usr/local/bin/tesseract

# Storage Configuration
NOTES_FOLDER=notes
TEMP_FOLDER=temp

# App Configuration
APP_TITLE=SAT/ACT Notes Organizer
APP_PORT=8501
DEBUG_MODE=false

# Optional: Obsidian Integration
OBSIDIAN_VAULT_PATH=

# Optional: Custom prompt templates
CUSTOM_SYSTEM_PROMPT=You are an expert tutor analyzing SAT/ACT study materials. Provide structured, helpful analysis.
